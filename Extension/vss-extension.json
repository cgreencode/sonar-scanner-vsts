{
    "manifestVersion":  1,
    "id":  "sonarqube-integration",
    "name":  "SonarQube Integration ",
    "version":  "1.0.0",
    "publisher":  "sonarsource",
    "targets":  [
                    {
                        "id":  "Microsoft.VisualStudio.Services"
                    }
                ],
    "description":  "Use the SonarQube Scanner analysis build tasks in your continuous integration builds to understand the technical debt of your projects.",
    "categories":  [
                       "Build and release"
                   ],
    "icons":  {
                  "default":  "extension-icon.png"
              },
    "tags":  [
                 "build",
                 "ci",
                 "continuous integration"
             ],
    "content":  {
                    "details":  {
                                    "path":  "overview.md"
                                },
                    "license":  {
                                    "path":  "license-terms.md"
                                }
                },
    "screenshots":  [

                    ],
    "links":  {
                  "home":  {
                               "uri":  "http://www.sonarqube.org/"
                           }
              },
    "branding":  {
                     "color":  "rgb(67, 157, 210)",
                     "theme":  "dark"
                 },
    "files":  [
                  {
                      "path":  "SonarQubeScannerMsBuildBegin"
                  },
                  {
                      "path":  "SonarQubeScannerMsBuildEnd"
                  }
              ],
    "contributions":  [
                          {
                              "id":  "EAC0BC9E-D29E-48F8-920F-4833DA6B513A",
                              "type":  "ms.vss-distributed-task.task",
                              "targets":  [
                                              "ms.vss-distributed-task.tasks"
                                          ],
                              "properties":  {
                                                 "name":  "SonarQubeScannerMsBuildBegin"
                                             }
                          },
                          {
                              "id":  "4F733B42-528E-4EE3-BCBC-2744511425F7",
                              "type":  "ms.vss-distributed-task.task",
                              "targets":  [
                                              "ms.vss-distributed-task.tasks"
                                          ],
                              "properties":  {
                                                 "name":  "SonarQubeScannerMsBuildEnd"
                                             }
                          },
                          {
                              "id":  "EDC775DD-1A82-4ADD-A073-7B5813E87202",
                              "description":  "Service Endpoint type for SonarQube Connections",
                              "type":  "ms.vss-endpoint.service-endpoint-type",
                              "targets":  [
                                              "ms.vss-endpoint.endpoint-types"
                                          ],
                              "properties":  {
                                                 "name":  "sonarqube",
                                                 "displayName":  "SonarQube Server",
                                                 "url":  {
                                                             "displayName":  "Server Url",
                                                             "helpText":  "Url for the SonarQube Server to connect to."
                                                         },
                                                 "dataSources":  [
                                                                     {
                                                                         "name":  "SonarQube Projects",
                                                                         "endpointUrl":  "{{endpoint.url}}api/projects/index",
                                                                         "resultSelector":  "jsonpath:$[*]"
                                                                     }
                                                                 ],
                                                 "authenticationSchemes":  [
                                                                               {
                                                                                   "type":  "ms.vss-endpoint.endpoint-auth-scheme-basic",
                                                                                   "inputDescriptors":  [
                                                                                                            {
                                                                                                                "id":  "username",
                                                                                                                "name":  "Token",
                                                                                                                "description":  "Authentication Token generated by SonarQube.",
                                                                                                                "inputMode":  "textbox",
                                                                                                                "validation":  {
                                                                                                                                   "isRequired":  true,
                                                                                                                                   "dataType":  "string"
                                                                                                                               }
                                                                                                            },
                                                                                                            {
                                                                                                                "id":  "password",
                                                                                                                "name":  "Password",
                                                                                                                "description":  "Password",
                                                                                                                "inputMode":  "passwordbox",
                                                                                                                "isConfidential":  true,
                                                                                                                "validation":  {
                                                                                                                                   "isRequired":  false,
                                                                                                                                   "dataType":  "string"
                                                                                                                               },
                                                                                                                "values":  {
                                                                                                                               "inputId":  "passwordInput",
                                                                                                                               "defaultValue":  "",
                                                                                                                               "isDisabled":  true
                                                                                                                           }
                                                                                                            }
                                                                                                        ]
                                                                               }
                                                                           ],
                                                 "helpMarkDown":  ""
                                             }
                          }
                      ]
}
